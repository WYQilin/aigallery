#videos-container {
  margin: 10rpx 0;
  min-height: 700rpx;
}

#videos-container button {
  margin: 0; 
  padding: 0; 
  background: rgb(0, 0, 0, 0);
}

#videos-container .video-image {
  width: 54rpx; 
  height: 54rpx;
  background: rgb(0, 0, 0, 0);
}

#videos-container button::after {
  border: 0px;
}

#videos-container .video-button-text {
  font-size:22rpx;
  font-family:PingFangSC-Semibold,PingFang SC;
  font-weight:600;
  color:#98999A;
  line-height:20px;
  margin-left: 4rpx;
  min-width: 44rpx;
  text-align: center;
}

.video>image {
  width: 100%; 
  background-image: url('data:image/png;base64,');
  background-size: 442rpx 164rpx;
  background-position: center;
  background-repeat: no-repeat;
}



.shanxing {
  width: 40rpx;
  height: 40rpx;
  position: relative;
  border-radius: 100rpx;
  overflow: hidden;
  display: flex;
  justify-content: center;
  align-items: center;
}

.sx1, .sx2 {
  position: absolute;
  top: -22;
  left: -22;
  width: 92rpx;
  height: 92rpx;
  clip: rect(0rpx, 46rpx, 92rpx, 0rpx);
}

.sx1 {
  z-index: 1;
}

.sx2 {
  z-index: 2;
}

.shanxing1 {
  background: #D8D8D8;
}

.shanxing1 .sx1, .shanxing1 .sx2 {
  background: #fff;
}

.shanxing2 {
  background: #fff;
}

.shanxing2 .sx1, .shanxing2 .sx2 {
  background: #D8D8D8;
  transform: rotate(180deg);
}